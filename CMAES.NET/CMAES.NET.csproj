<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CMAESnet</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Authors>yn-cloud, VassilSanych</Authors>
    <Company />
    <Product />
    <!--<RepositoryUrl>https://github.com/yn-cloud/CMAES.NET</RepositoryUrl>-->
    <PackageTags>CMA-ES Optimization Black-box-optimization</PackageTags>
    <Copyright />
    <!--<PackageProjectUrl>https://github.com/yn-cloud/CMAES.NET</PackageProjectUrl>-->
    <Description>Covariance Matrix Adaptation Evolution Strategy (CMA-ES) implementation on .NET</Description>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MathNet.Numerics" Version="4.15.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
